<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
<mapper namespace="com.swu.mapper.StudyVideoMapper">
	<insert id="insertUrl" parameterType="com.swu.vo.StudyVideo">
		INSERT INTO youtubeTest(url) VALUES (#{url})
	</insert>
	<select id="selectUrl" resultType="com.swu.vo.StudyVideo">
		SELECT url FROM youtubeTest
	</select>
</mapper>